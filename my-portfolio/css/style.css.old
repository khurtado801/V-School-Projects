/* Add Animation */
.img-wrapper {
  width: 100%;
  height: 75%;
  
}

.modal-content{
  margin: auto;
  display: block;
  width: 100%;
  height: 70%;
  max-width: 700px;
  /* overflow: auto; */
  -webkit-animation-name: zoom;
  -webkit-animation-duration: 0.6s;
  animation-name: zoom;
  animation-duration: 0.6s;
  pointer-events: none;
}
#modal-content, #caption {    
  -webkit-animation-name: zoom;
  -webkit-animation-duration: 0.6s;
  animation-name: zoom;
  animation-duration: 0.6s;
}
/*  */

.modalDialog {
  padding-top: 50px;
  position: fixed;
  width: 100%;
  height:100%;
  top: 20px;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0,0,0,0.8);
  z-index: 99999;
  opacity:0;
  -webkit-animation-name: zoom;
  -webkit-animation-duration: 0.6s;
  animation-name: zoom;
  animation-duration: 0.6s;
  pointer-events: none;
}

.modalDialog:target {
  opacity:1;
  pointer-events: auto;
}

.modalDialog > div {
  position: relative;
  /* margin: 10% auto; */
  padding: 5px 20px 13px 20px;
  border-radius: 10px;
  background: #fff;
  margin: auto;
  display: block;
  width: 80%;
  max-width: 700px;
  background: -moz-linear-gradient(#fff, #999);
  background: -webkit-linear-gradient(#fff, #999);
  background: -o-linear-gradient(#fff, #999);
  
}

.close {
  z-index: 99999;
  /* opacity:-5; */
  background: #00d9ff;
  color: #FFFFFF;
  line-height: 25px;
  position: absolute;
  right: -12px;
  text-align: center;
  top: -10px;
  width: 24px;
  text-decoration: none;
  font-weight: bold;
  -webkit-border-radius: 12px;
  -moz-border-radius: 12px;
  border-radius: 12px;
  -moz-box-shadow: 1px 1px 3px #000;
  -webkit-box-shadow: 1px 1px 3px #000;
  box-shadow: 1px 1px 3px #000;
}

.close:hover { background: #00d9ff; }



/*  */


@-webkit-keyframes zoom {
  from {-webkit-transform:scale(0)} 
  to {-webkit-transform:scale(1)}
}

@keyframes zoom {
  from {transform:scale(0)} 
  to {transform:scale(1)}
}

/* The Close Button */
.close-btn {
  background: #00d9ff;
  position: absolute;
  top: 15px;
  right: 35px;
  color: #fff;
  font-size: 40px;
  font-weight: bold;
  transition: 0.3s;
  text-shadow: 0 1px 0 rgb(237, 234, 234), 0 2px 0 #c9c8c8, 0 3px 0 rgb(179, 177, 177), 0 4px 0 #cbcaca, 0 5px 0 rgb(216, 214, 214);
}

#close-btn:hover,
#close-btn:focus {
  color: #bbb;
  text-decoration: none;
  cursor: pointer;
}

/* 100% Image Width on Smaller Screens */
@media only screen and (max-width: 700px){
  .modal-content {
      width: 100%;
  }
}